{
  "name": "server",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "build": "webpack",
    "build:prod": "NODE_ENV=production webpack --mode production",
    "start": "node dist/server.js",
    "start:dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon build/server.js\" \"npm run swagger\"",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "swagger": "tsoa spec",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --ignore-path .gitignore --write \"./src/**/*.+(js|ts|json)\"",
    "prettier:fix": "prettier --write src"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "description": "",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.12.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^6.6.0",
    "webpack-node-externals": "^3.0.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.4",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "babel-preset-env": "^0.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.20.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.1",
    "superagent": "^10.1.1",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3",
    "webpack-cli": "^6.0.1"
  }
}
